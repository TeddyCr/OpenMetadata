{
    "$id": "https://open-metadata.org/schema/analytics/basic.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Basic",
    "description": "This schema defines basic types that are used by analytics classes",
    "definitions": {
      "reportResultValue": {
        "description": "Schema to capture test case result values.",
        "javaType": "org.openmetadata.schema.analytics.type.ReportResultValue",
        "type": "object",
        "properties": {
          "name": {
            "description": "name of the value",
            "type": "string"
          },
          "value": {
            "description": "report result value",
            "type": "string"
          }
        }
      },
      "reportResult": {
        "description": "Schema to capture report result values.",
        "javaType": "org.openmetadata.schema.analytics.type.ReportResult",
        "type": "object",
        "properties": {
          "timestamp": {
            "description": "Data one which test case result is taken.",
            "$ref": "../type/basic.json#/definitions/timestamp"
          },
          "dimensions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/reportResultValue"
            }
          },
          "metrics": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/reportResultValue"
            }
          }
        }
      }
    }
  }