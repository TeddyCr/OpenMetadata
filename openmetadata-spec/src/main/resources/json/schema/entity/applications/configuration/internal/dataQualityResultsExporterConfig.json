  },
  "properties": {
    "type": {
      "title": "Application Type",
      "description": "Application Type",
      "$ref": "#/definitions/dataQualityResultsExporterAppType",
      "default": "DataInsightsReport"
    },
    "config": {
      "mask": true,
      "oneOf": [
        {
          "$ref": "./connections/snowflakeConnection.json"
        }
      ],
      "default": null
    },
    "databaseName": {
        "type": "string",
        "description": "Name of the database where the data quality results are stored."
    },
    "schemaName": {
        "type": "string",
        "description": "Name of the schema where the data quality results are stored."
    },
    "tableName": {
        "type": "string",
        "description": "Name of the table where the data quality results are stored."
    }
  },
  "additionalProperties": false
